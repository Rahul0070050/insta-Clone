<section class="user-section">
    <div class="container user-home-page">
        <div class="row">
            <div class="col-md-7">
                <div class="status-container py-2 my-5">
                    <div class="status mx-2">
                        <div class="imgd">
                            <img src="data:image/png;base64,"
                                alt="">
                        </div>
                        <p>Name</p>
                    </div>
                    <div class="status mx-2">
                        <div class="imgd">
                            <img src="data:image/png;base64,"
                                alt="">
                        </div>
                        <p>Name</p>
                    </div>
                </div>
                {{#if allPost}}
                {{#each allPost}}
                <div class="user-card my-3">
                    <div class="user-header px-2 p-2">
                        <div class="name-img">
                            {{#if profile}}
                            <img src="/userProile/{{this.userId}}.jpg" alt="">
                            {{else}}
                            <img src="data:image/png;base64,"
                                alt="">
                            {{/if}}
                            <a href="/show-user/{{this.userId}}">{{this.post.name}}</a>
                        </div>
                        <i class="far fa-ellipsis-h fa-2x"></i>
                    </div>
                    <div class="user-post">
                        <img src="/userPost/post/{{this.post.img}}.jpg" alt="">
                    </div>
                    <div class="user-erea p-3">
                        <div class="users-opretion">
                            <div class="user-reacti{on">
                                {{!-- now adding like fun --}}
                                <span id="{{this.post.img}}">
                                    {{#if liked}}
                                    <a onclick="removeLike('{{this.post.img}}','{{this.userId}}','{{../user._id}}')"><i
                                            id="red" class="fas fa-heart fa-2x"></i></a>
                                    {{else}}
                                    <a onclick="addLike('{{this.post.img}}','{{this.userId}}','{{../user._id}}')"><i
                                            class="far fa-heart fa-2x"></i></a>
                                    {{/if}}
                                </span>
                                <a><i class="fal fa-comment fa-2x mx-3"></i></a>
                                <a><i class="fad fa-paper-plane text-dark fa-2x"></i></a>
                            </div>
                            <a><i id="black" class="far fa-bookmark fa-2x"></i></a>
                        </div>
                        <div class="like py-1">
                            <h6><span id="{{this.post.img}}{{this.post.img}}">{{this.likesCound}}</span> Likes</h6>
                        </div>
                        <div class="description">
                            <h6><span class="text-muted">{{this.post.discription}}</span></h6>
                        </div>
                        <div id="{{this.post.img}}{{this.post.img}}{{this.post.img}}">
                            {{#each comments}}
                            <h6>{{this.username}} <span class="text-muted">{{this.comment}}</span></h6>
                            {{/each}}
                            <a style="color: #007bff; cursor: pointer;"
                                onclick="getComments('{{this.post.img}}','{{this.userId}}')">more...</a>
                        </div>
                        <hr>
                        <div class="comments-input-feeld">
                            <input type="text" name="comment"
                                id="{{this.post.img}}{{this.post.img}}{{this.post.img}}{{this.post.img}}"
                                placeholder="Add a comment">
                            <a style="color: #007bff; cursor: pointer;"
                                onclick="postComment('{{this.post.img}}','{{this.userId}}')">post</a>
                        </div>
                    </div>
                </div>
                {{/each}}
                {{else}}
                <h3>No Post</h3>
                {{/if}}
            </div>
            <div class="col-md-5 details-user mt-5">
                <div class="mutchel-friends ml-5">
                    <div class="user">
                        {{#if user.profile}}
                        <img src="/userProile/{{user._id}}.jpg" alt="">
                        {{else}}
                        <img src="data:image/png;base64,"
                            alt="">
                        {{/if}}
                        <div class="user-details ml-3">
                            <div class="user-loged">
                                <a href="/userpanul">
                                    <h6>{{user.fullname}}</h6>
                                </a>
                                <p class="text-muted">{{user.username}}</p>
                            </div>
                            <a href="">Switch</a>
                        </div>
                    </div>
                    <div class="suggetions mt-3">
                        <div class="suggetions-for-you">
                            <h6 class="text-muted">Suggetions For You</h6>
                            <a href="">See all</a>
                        </div>
                        {{#each allUsers}}
                        <div class="sugetions-list mt-3">
                            {{#if this.profile}}
                            <img src="/userProile/{{this._id}}.jpg" alt="">
                            {{else}}
                            <img src="data:image/png;base64,"
                                alt="">
                            {{/if}}
                            <div class="msugetion-users">
                                <div class="mutul-friends ml-3">
                                    <a href="/show-user/{{this._id}}">{{this.username}}</a>
                                    <p>{{this.fullname}}</p>
                                </div>
                                <span id="{{this._id}}">
                                    <a style="color: #007bff;" onclick="addFriendHomeScren('{{this._id}}')">Follow</a>
                                </span>
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>


<script>

    addFriendHomeScren = (userId) => {
        var cound;
        fetch("/addFriend", {
            method: "POST",
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                foundUser: userId
            })
        })
            .then(response => response.json())
            .then(data => {
                document.getElementById(userId).innerHTML = `
                <a style="color: #007bff;" onclick="removeFriendFromHome('${userId}')" >Unfollow</a>
                `
            })
    }

    removeFriendFromHome = (userId) => {
        fetch("/deleteUser", {
            method: "POST",
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                userId: userId
            })
        })
            .then(response => response.json())
            .then(data => {
                if (data.foundUserFolloversCound == 0) {
                }
            })
        document.getElementById(userId).innerHTML = `
        <a style="color: #007bff;" onclick="addFriendHomeScren('${userId}')" mt-1 mr-2 ml-auto py-1 px-4">Follow</a>
        `
    }


    addLike = (imgId, userId, id) => {
        let data = new FormData()
        data.append('img', imgId)
        data.append('userId', userId)
        data.append('Id', id)
        fetch("/addLike", {
            method: 'post',
            body: data
        })
            .then(res => res.json())
            .then(data => {
                document.getElementById(imgId + imgId).innerHTML = data.likesCound
                document.getElementById(imgId).innerHTML = `
                    <a onclick="removeLike('${imgId}','${userId}','${id}')"><i id="red" class="fas fa-heart fa-2x"></i></a>
                `
            })
    }



    removeLike = (imgId, userId, id) => {
        let data = new FormData()
        data.append('img', imgId)
        data.append('userId', userId)
        data.append('Id', id)
        fetch("/removeLike", {
            method: 'post',
            body: data
        })
            .then(res => res.json())
            .then(data => {
                document.getElementById(imgId + imgId).innerHTML = data.likesCound
                document.getElementById(imgId).innerHTML = `
                    <a onclick="addLike('${imgId}','${userId}','${id}')"><i class="far fa-heart fa-2x"></i></a>
                `
            })
    }


    postComment = (imgId, userId) => {
        let data = new FormData()
        let comment = document.getElementById(imgId + imgId + imgId + imgId).value;
        data.append('img', imgId);
        data.append('userId', userId);
        data.append("comment", comment)

        fetch("/add_comment", {
            method: 'post',
            body: data
        })
            .then(res => res.json())
            .then(data => {
                document.getElementById(imgId + imgId + imgId + imgId).value = ""
                document.getElementById(imgId + imgId + imgId).innerHTML = `
                    <h6>${data.comment.username} <span class="text-muted">${data.comment.comment}</span></h6>
                    <a style="color: #007bff; cursor: pointer;" onclick="getComments('${imgId}','${userId}')">more...</a>
                `
            })
    }


    getComments = (imgId, userId) => {
        let data = new FormData()
        data.append('img', imgId);
        data.append('userId', userId);

        fetch("/get_comments", {
            method: 'post',
            body: data
        })
            .then(res => res.json())
            .then(data => {
                let comment = ""
                for (var i = 0; i < data.length; i++) {
                    comment += `
                        <h6>${data[i].username} <span class="text-muted">${data[i].comment}</span></h6>
                    `
                }
                document.getElementById(imgId + imgId + imgId).innerHTML =
                    `
                    ${comment}
                    <a style="color: #007bff; cursor: pointer;" onclick="hideComment('${imgId}','${userId}')">hide</a>
                `
            })
    }


    hideComment = (imgId, userId) => {
        let data = new FormData()
        data.append('img', imgId);
        data.append('userId', userId);

        fetch("/hide_comments", {
            method: 'post',
            body: data
        })
            .then(res => res.json())
            .then(data => {
                console.log(data)
                let comment = ""
                for (var i = 0; i < 1 ; i++) {
                    comment += `
                        <h6>${data[0].username} <span class="text-muted">${data[0].comment}</span></h6>
                    `
                }
                document.getElementById(imgId + imgId + imgId).innerHTML =
                    `
                    ${comment}
                    <a style="color: #007bff; cursor: pointer;" onclick="getComments('${imgId}','${userId}')">more...</a>
                    `
            })
    }


</script>