<section class="find-user-sectiuon">
    <div class="container">
        <div class="row">
            <div class="user-panul">
                <div class="user-information d-flex px-5 mt-3">
                    <div class="user-profile-photo ml-auto">
                        {{#if profile}}
                        <img src="/userProile/{{userId}}.jpg" alt="">
                        {{else}}
                        <img src="data:image/png;base64,"
                            alt="">
                        {{/if}}
                    </div>
                    <div class="user-full-details">
                        <div class="user-profile-name d-flex flex-column">
                            <h4>{{userName}}</h4>
                            <h6>{{emailId}}</h6>
                        </div>
                        <div class="user-score d-flex mt-2">
                            <h5 class="mr-3">{{findUserPostCound}}<span class="text-muted"
                                    id="post-cound">{{allPostCound}}</span> Post</h5>
                            <h5 href="">
                                <h5 class="ml-3 mr-1"><span class="text-muted Followers-cound"
                                        id="found-User-Followers">{{followersCound}}</span><a
                                        class="find-user-followers" onclick="getFollowers('{{userId}}','{{id}}')">
                                        Followers</a>
                                </h5>
                            </h5>
                            <h5>
                                <h5 class="ml-4"><span class="text-muted"
                                        id="found-User-Following">{{followingCound}}</span><a
                                        class="find-user-followings" onclick="getFollowings('{{userId}}','{{id}}')">
                                        Following</a></h5>
                            </h5>
                        </div>
                        <div class="follow-buttons">
                            {{#if friend}}
                            <button onclick="removeFriend('{{userId}}')"
                                class="btn btn-danger mt-1 mr-2 ml-auto py-1 px-4">Unfollow</button>
                            <button onclick="message('{{userId}}')" class="btn btn-success mt-1  ml-auto py-1 px-4"
                                id="show-ig">Message</button>
                            {{else}}
                            <button onclick="addFriend('{{userId}}')"
                                class="btn btn-success mt-1 mr-2 ml-auto py-1 px-4">Follow</button>
                            {{/if}}
                        </div>
                    </div>
                </div>
                <div class="user-post">
                    <div class="user-post-options mt-4">
                        <a href="" class="text-muted">
                            <h5>POSTS</h5>
                        </a>
                        <a href="" class="text-muted">
                            <h5>IGTV</h5>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{!-- =====================================foloowers and following=================================== --}}
    <div class="user-friends-show hiden">
        <span class="cls" id="closs-ID">X</span>
        <div class="followers-head text-center">
            <h2>{{userName}}</h2>
        </div>
        <hr>
        <div class="followers-options">
            <button class="btn" onclick="getFollowers('{{userId}}','{{id}}')">Followers</button>
            <button class="btn" onclick="getFollowings('{{userId}}','{{id}}')">Following</button>
        </div>
        <div class="follovers-body" id="follover-body">
            {{!-- ================== --}}
            {{!-- ===================== --}}
        </div>
    </div>
    {{!-- =====================================foloowers and following=================================== --}}
</section>
<section class="post-view-section">
    <div class="container py-3">
        <div class="row" id="post-view-imgs-rop">
            {{#each allPost}}
            <div class="m-3 my-3">
                <div id="post">
                    <img src="userPost/post/{{this.post.img}}.jpg" alt="">
                    {{!-- <a href="" class="post-delet-button"><i class="fas fa-trash img-trash"></i></a> --}}
                    <div id="post-descreption">
                        <h6>{{this.post.discription}}</h6>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</section>





