<section>
    <div class="container">
        <div class="row">
            <div class="user-panul">
                <div class="user-information d-flex px-5 mt-3">
                    <div class="user-profile-photo ml-auto">
                        {{#if profile}}
                        <img src="/userProile/{{user._id}}.jpg" alt="">
                        {{else}}
                        <img src="data:image/png;base64,"
                            alt="">
                        {{/if}}
                    </div>
                    <div class="user-full-details">
                        <div class="user-profile-name">
                            <h2>{{fullname}}</h2>
                            <a href="/edit-user-profil" class="profile-edit">Edit Profile</a>
                            <i class="fa fa-cog m-2"></i>
                        </div>
                        <h6>{{emailId}}</h6>

                        <div class="user-score  d-flex mt-2">
                            <h5 class="mr-3"><span class="text-muted" id="post-cound">{{allPostCound}}</span> Post</h5>
                            <h5 href="">
                                <h5 class="ml-3 mr-1"><span class="text-muted"
                                        id="found-User-Followers">{{followersCound}}</span> <a onclick="getFollowers()"
                                        style="cursor: pointer;" id="followers">Followers</a>
                                </h5>
                            </h5>
                            <h5>
                                <h5 class="ml-4"><span class="text-muted" id="following-cound">{{followingCound}}</span>
                                    <a onclick="getFollowings()" style="cursor: pointer;" id="following">Following</a>
                                </h5>
                            </h5>
                        </div>
                        <button class="btn btn-success mt-3 ml-auto" id="show-post">Make a Post</button>
                        <button class="btn btn-success mt-3 ml-auto" id="show-ig">Make a IGTV</button>
                    </div>
                </div>
                <div class="user-post">
                    <div class="user-post-options mt-4">
                        <a href="" class="text-muted">
                            <h5>POSTS</h5>
                        </a>
                        <a href="" class="text-muted">
                            <h5>IGTV</h5>
                        </a>
                        <a href="" class="text-muted">
                            <h5>SAVED</h5>
                        </a>
                    </div>
                </div>
            </div>
            {{!-- ==========post-form============= --}}
            <div class="making-post hiden" id="post">
                <form id="poosting-form" method="POST" enctype="multipart/form-data">
                    <div class="closs">
                        <h3 class="text-muted m-3" id="postClossBtn">X</h3>
                    </div>
                    <div class="title mt-4 pt-2">
                        <h3>Make Your On Post</h3>
                    </div>
                    <div class="form p-4">
                        <label for="">Seclect Post</label>
                        <input type="file" name="postImg" class="form-control" id="postFileSpace">
                        <label for="">Description</label>
                        <textarea type="text" rows="5" cols="50" name="postDescription" id="description"
                            class="post-control" id=""></textarea>
                        <button class="btn btn-success my-2" onclick="post_get_Cound()" type="button"
                            id="posts">POST</button>
                    </div>
                </form>
            </div>
            {{!-- ==========igtv-form============= --}}
            <div class="making-post hiden" id="igtv">
                <form id="poosting-form" method="POST" enctype="multipart/form-data">
                    <div class="closs">
                        <h3 class="text-muted m-3" id="igClossBtn">X</h3>
                    </div>
                    <div class="title mt-4 pt-2">
                        <h3>Make Your On Ig</h3>
                    </div>
                    <div class="form p-4">
                        <label for="">Seclect Post</label>
                        <input type="file" name="IgPost" class="form-control" id="IgFileSpace">
                        <label for="">Description</label>
                        <textarea type="text" rows="5" cols="50" name="igDescription" class="ig-control"
                            id=""></textarea>
                        <button class="btn btn-success my-2" id="ig">POST</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    {{!-- ================================Followers-list-start====================================== --}}
    <div class="user-friends-show hiden">
        <span class="cls" id="closs-id">X</span>
        <div class="followers-head">
            <div class="search">
                <div class="followers-head text-center">
                    <h2>{{userName}}</h2>
                </div>
            </div>
        </div>
        <hr>
        <div class="followers-options">
            <button class="btn" onclick="getFollowers()">Followers</button>
            <button class="btn" onclick="getFollowings()">Following</button>
        </div>
        <div class="follovers-body" id="follover-body">
            {{!-- ================== --}}
            {{!-- ===================== --}}
        </div>
    </div>
</section>
{{!-- ================================Followers-list-end====================================== --}}
<section class="post-view-section">
    <div class="container py-3">
        <div class="row" id="post-view-imgs-rop">
            {{#each allPost}}
            <div class="m-3 my-3" onclick="">
                <div id="post">
                    <img src="/userPost/post/{{this.post.img}}.jpg" alt="">
                    <button onclick="post_delet('{{this.post.img}}')" class="post-delet-button"><i
                            class="fas fa-trash img-trash"></i></button>
                    <div id="post-descreption">
                        <h6>{{this.post.discription}}</h6>
                    </div>
                </div>
            </div>
            {{/ each}}
        </div>
    </div>
</section>



