<script>
    /*===============Post-showing and cound-autoUpdating=================*/
    // POSR UPLOADING AND -COUND
    post_get_Cound = () => {
        const formData = new FormData()
        let img = document.querySelector('input[type="file"]');
        let description = document.getElementById('description').value;
        //Storing the data like key value paire
        formData.append('img', img.files[0])
        formData.append('description', description);

        fetch("/user-post", {
            method: 'POST',
            body: formData
        })
            .then(response => response.json())
            .then(data => {
                console.log(data)
                let allPost = data
                if (allPost.length == 0) {
                    document.querySelector("#post-cound").innerHTML = "0"
                } else if (allPost.length == 1) {
                    document.querySelector("#post-cound").innerHTML = "1"
                }
                document.querySelector("#post-cound").innerHTML = allPost.length
                let posts = "";
                for (var i = 0; i < allPost.length; i++) {
                    posts +=
                        `<div class="m-3 my-3">
                            <div id="post">
                                <img src="/userPost/post/${allPost[i].post.img}.jpg" alt="">
                                <button onclick="post_delet('${allPost[i].post.img}')" class="post-delet-button"><i
                                        class="fas fa-trash img-trash"></i></button>
                                <div id="post-descreption">
                                    <h6>${allPost[i].post.discription}</h6>
                                </div>
                            </div>
                        </div>`
                    document.getElementById("post-view-imgs-rop").innerHTML = posts
                }
            })
            .catch(err => {
                alert(err)
            })
    }

    /*===============Post-showing and cound-autoUpdating -END- =================*/


    /*=================Post deleting autoUpdating cound=================================*/
    //POST DELETE AND -COUND
    post_delet = (postId) => {
        const formData = new FormData()
        formData.append('postId', postId)
        fetch("/post_delet", {
            method: "post",
            body: formData
        })

            .then(response => response.json())
            .then(data => {
                let allPost = data
                if (allPost == 0) {
                    document.querySelector("#post-cound").innerHTML = "0"
                } else if (allPost.length == 1) {
                    document.querySelector("#post-cound").innerHTML = "1"
                }
                document.querySelector("#post-cound").innerHTML = allPost.length
                let posts = "";
                for (var i = 0; i < allPost.length; i++) {
                    posts +=
                        `<div class="m-3 my-3">
                            <div id="post">
                                <img src="/userPost/post/${allPost[i].post.img}.jpg" alt="">
                                <button onclick="post_delet('${allPost[i].post.img}')" class="post-delet-button"><i
                                        class="fas fa-trash img-trash"></i></button>
                                <div id="post-descreption">
                                    <h6>${allPost[i].post.discription}</h6>
                                </div>
                            </div>
                        </div>`
                    document.getElementById("post-view-imgs-rop").innerHTML = posts
                }
            })
            .catch(err => {
                alert(err)
            })
    }

    /*=================Post deleting autoUpdating cound -END- =================================*/


    /*==================================geting follovers=====================================*/
    getFollowers = () => {
        var folovers = ""
        fetch("/get-all-followers", {
            method: 'get'
        })
            .then(response => response.json())
            .then(data => {
                if (data.length == 0) {
                    document.getElementById("follover-body").innerHTML = ""
                }
                for (var i = 0; i < data.length; i++) {
                    if (data[i].Im_following) {
                        if (data[i].profile) {
                            folovers += `
                            <div class="follovers">
                                <div class="img">
                                    <img src="/userProile/${data[i]._id}.jpg"
                                        alt="">
                                    <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                                </div>
                                <div class="followers-data">
                                    <span id="${data[i]._id}">
                                        <a onclick="removeFriendFromfollovers('${data[i]._id}')">Following</a>
                                    </span>
                                    <a onclick="removeFollover('${data[i]._id}')">Remove</a>
                                </div>
                            </div>
                        `
                        } else {
                            folovers += `
                                <div class="follovers">
                                    <div class="img">
                                        <img src="data:image/png;base64,"
                                            alt="">
                                        <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                                    </div>
                                    <div class="followers-data">
                                        <span id="${data[i]._id}">
                                            <a onclick="removeFriendFromfollovers('${data[i]._id}')">Following</a>
                                        </span>
                                        <a onclick="removeFollover('${data[i]._id}')">Remove</a>
                                    </div>
                                </div>
                                `
                        }
                        document.getElementById("follover-body").innerHTML = folovers
                    } else {
                        if (data[i].profile) {
                            folovers += `
                        <div class="follovers">
                            <div class="img">
                                <img src="/userProile/${data[i]._id}.jpg"
                                    alt="">
                                <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                            </div>
                            <div class="followers-data">
                                <span id="${data[i]._id}">
                                    <a onclick="addFollowerFromFollovers('${data[i]._id}')">Follow</a>
                                </span>
                                <a onclick="removeFollover('${data[i]._id}')" >Remove</a>
                            </div>
                        </div>
                        `
                        } else {
                            folovers += `
                                <div class="follovers">
                                    <div class="img">
                                        <img src="data:image/png;base64,"
                                            alt="">
                                        <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                                    </div>
                                    <div class="followers-data">
                                        <span id="${data[i]._id}">
                                            <a onclick="addFollowerFromFollovers('${data[i]._id}')">Follow</a>
                                        </span>
                                        <a onclick="removeFollover('${data[i]._id}')" >Remove</a>
                                    </div>
                                </div>
                                `
                        }

                        document.getElementById("follover-body").innerHTML = folovers
                    }
                }

            })
    }

    /*==================================geting follovers -end -=====================================*/


    /*============================geting all followers -start- ======================*/
    getFollowings = () => {
        fetch("/get-following", {
            method: 'get',
            headers: {
                'Content-Type': 'Application/json'
            },

        })
            .then(res => res.json())
            .then(data => {
                var allFollowers = ""
                if (data.length == 0) {
                    document.getElementById("follover-body").innerHTML = ""
                }
                for (var i = 0; i < data.length; i++) {
                    if (data[i].Im_following) {
                        if (data[i].profile) {
                            allFollowers += `
                        <div class="follovers">
                            <div class="img">
                                <img src="/userProile/${data[i]._id}.jpg"
                                    alt="">
                                <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                            </div>
                            <div class="followers-data">
                                <span id="${data[i]._id}">
                                    <a onclick="removeFriendFromfollovers('${data[i]._id}')">Following</a>
                                </span>
                            </div>
                        </div>
                        `
                        } else {
                            allFollowers += `
                        <div class="follovers">
                            <div class="img">
                                <img src="data:image/png;base64,"
                                    alt="">
                                <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                            </div>
                            <div class="followers-data">
                                <span id="${data[i]._id}">
                                    <a onclick="removeFriendFromfollovers('${data[i]._id}')">Following</a>
                                </span>
                            </div>
                        </div>
                        `
                        }

                        document.getElementById("follover-body").innerHTML = allFollowers
                    } else {
                        if (data[i].profile) {
                            allFollowers += `
                        <div class="follovers">
                            <div class="img">
                                <img src="/userProile/${data[i]._id}.jpg"
                                    alt="">
                                <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                            </div>
                            <div class="followers-data">
                                <span id="${data[i]._id}">
                                    <a onclick="addFollowerFromFollovers('${data[i]._id}')">Follow</a>
                                </span>
                            </div>
                        </div>
                        `
                        } else {
                            allFollowers += `
                        <div class="follovers">
                            <div class="img">
                                <img src="data:image/png;base64,"
                                    alt="">
                                <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                            </div>
                            <div class="followers-data">
                                <span id="${data[i]._id}">
                                    <a onclick="addFollowerFromFollovers('${data[i]._id}')">Follow</a>
                                </span>
                            </div>
                        </div>
                        `
                        }
                        document.getElementById("follover-body").innerHTML = allFollowers
                    }
                }
            })
    }

    /*============================geting all followers -end- ======================*/




    /*======================== unfollow btn -start- ============================*/
    removeFriendFromfollovers = (userId) => {
        fetch("/remove-fround-from-user-panul", {
            method: "POST",
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                userId: userId
            })
        })
            .then(response => response.json())
            .then(data => {
                if (data == 0) {

                }
                document.getElementById("following-cound").innerHTML = data.length
            })
        document.getElementById(userId).innerHTML = `
            <a onclick="addFollowerFromFollovers('${userId}')">Follow</a>
        `
    }

    /*======================== unfollow btn end- ============================*/


    /*===========================follo action -start- ============================*/
    addFollowerFromFollovers = (userId) => {
        fetch("/follow-friend-from-user-panul", {
            method: "POST",
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({
                foundUser: userId
            })
        })
            .then(response => response.json())
            .then(data => {
                document.getElementById("following-cound").innerHTML = data.length
            })
        document.getElementById(userId).innerHTML = `
                <a onclick="removeFriendFromfollovers('${userId}')">Following</a>
            `
    }


    const closeFollowers = document.getElementById("closs-id")
    const friendsDiv = document.querySelector(".user-friends-show")
    const shoFollowing = document.getElementById("following")
    const showFfollowers = document.getElementById("followers")

    showFfollowers.addEventListener("click", () => {
        friendsDiv.classList.toggle("hiden")
    })
    shoFollowing.addEventListener("click", () => {
        friendsDiv.classList.toggle("hiden")
    })

    closeFollowers.addEventListener("click", () => {
        friendsDiv.classList.toggle("hiden")
    })

    /*===========================follo action -end- ============================*/




    /*===========================remove follower -start- ============================*/
    removeFollover = (userId) => {
        fetch("/remove-follower", {
            method: 'post',
            body: JSON.stringify({
                userId: userId
            }),
            headers: {
                'Content-Type': 'Application/json'
            }
        })
            .then(res => res.json())
            .then(data => {
                if (data.length == 0) {
                    document.getElementById("follover-body").innerHTML = ""
                    document.getElementById("found-User-Followers").innerHTML = 0
                } else {
                    document.getElementById("found-User-Followers").innerHTML = data.length
                }
                var folovers = ""
                for (var i = 0; i < data.length; i++) {
                    if (data[i].Im_following) {
                        folovers += `
                        <div class="follovers">
                            <div class="img">
                                <img src="data:image/png;base64,"
                                    alt="">
                                <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                            </div>
                            <div class="followers-data">
                                <span id="${data[i]._id}">
                                    <a onclick="removeFriendFromfollovers('${data[i]._id}')">Following</a>
                                </span>
                                <a onclick="removeFollover('${data[i]._id}')">Remove</a>
                            </div>
                        </div>
                        `
                        document.getElementById("follover-body").innerHTML = folovers
                    } else {
                        folovers += `
                        <div class="follovers">
                            <div class="img">
                                <img src="data:image/png;base64,"
                                    alt="">
                                <a href="/show-user/${data[i]._id}" >${data[i].username}</a>
                            </div>
                            <div class="followers-data">
                                <span id="${data[i]._id}">
                                    <a onclick="addFollowerFromFollovers('${data[i]._id}')">Follow</a>
                                </span>
                                <a onclick="removeFollover('${data[i]._id}')" >Remove</a>
                            </div>
                        </div>
                        `
                        document.getElementById("follover-body").innerHTML = folovers
                    }
                }
            })
    }
    /*===========================remove follower -end- ============================*/

</script>